generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model TypingTest {
  id             Int      @id(map: "PK_cb119115f89df3bdbbfa207ae5a") @default(autoincrement())
  wordsTyped     String
  timeToComplete Float
  rawWpm         Int
  accuracy       Int
  wpm            Int
  date           DateTime @db.Date
  userId         String   @db.Uuid
  @@map("typing_test")
}

model User {
  id         String   @id(map: "PK_a3ffb1c0c8416b9fc6f907b7433") @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  email      String   @unique(map: "UQ_97672ac88f789774dd47f7c8be3")
  firebaseId String   @unique(map: "UQ_1c2d0bf809fd5f537a43d8f8d8e")
  dateJoined DateTime @db.Date
  @@map("users")
}
